    "egg-redis": "^2.2.0",
    "egg-scripts": "^2.10.0",
    "egg-sequelize": "^4.2.0",
    "egg-session-redis": "^2.1.0",
    "egg-socket.io": "^4.1.5",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.2.0",
    "js-md5": "^0.7.3",
    "koa-compress": "^3.0.0",
    "mysql2": "^1.5.2",
    "stream-to-array": "^2.3.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
